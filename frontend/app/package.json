{
  "name": "investoko-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "standard": "npx standard",
    "analyze": "ANALYZE=true npm run build",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@next/bundle-analyzer": "^9.2.2",
    "@zeit/next-sass": "^1.0.1",
    "next": "10.0.3",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "devDependencies": {
    "docz": "^2.2.0",
    "husky": "^4.3.0",
    "standard": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx standard"
    }
  },
  "standard": {
    "globals": [
      "require",
      "React"
    ],
    "ignore": [
      "node_modules"
    ],
    "fix": true
  }
}
